div.au-month-picker-content
  div.au-date-picker-title-bar
    .au-date-picker-button(@click="prevYear")
      au-icon(icon="angle-double-left")

    .au-date-picker-text
      div.au-date-picker-text-button(@click="showYearPanel")|{{ year }}å¹´

    .au-date-picker-button(@click="nextYear")
      au-icon(icon="angle-double-right")

  div.au-date-picker-content
    .au-month-picker-list
      .au-month-picker-item.au-date-picker-item(:class="{ 'au-text-current': currentMonth == month }", v-for="month in months", @click="setMonth(month)")|{{month}}